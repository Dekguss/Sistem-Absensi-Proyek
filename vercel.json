{
  "version": 2,
  "builds": [
    {
      "src": "public/index.php",
      "use": "@vercel/php"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/public/$1"
    }
  ],
  "build": {
    "env": {
      "APP_ENV": "production",
      "NODE_VERSION": "16.x"
    }
  },
  "buildCommand": "npm install && npm run production",
  "installCommand": "npm install",
  "framework": null,
  "outputDirectory": "public"
}