{
  "version": 2,
  "functions": {
    "api/index.php": {
      "runtime": "vercel-php@0.6.0"
    }
  },
  "routes": [
    { "src": "/(.*)", "dest": "/api/index.php" }
  ],
  "build": {
    "env": {
      "APP_ENV": "production",
      "NODE_VERSION": "16.x"
    }
  },
  "buildCommand": "npm install && npm run production",
  "installCommand": "npm install",
  "framework": null,
  "outputDirectory": "public"
}