{
  "version": 2,
  "builds": [
    {
      "src": "public/index.php",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "public"
      }
    }
  ],
  "routes": [
    { "src": "/(.*)", "dest": "/public/$1" }
  ],
  "build": {
    "env": {
      "APP_ENV": "production"
    }
  },
  "buildCommand": "npm install && npm run production",
  "installCommand": "npm install",
  "framework": null,
  "outputDirectory": "public"
}