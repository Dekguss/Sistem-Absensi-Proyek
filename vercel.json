{
  "version": 2,
  "builds": [
    {
      "src": "public/index.php",
      "use": "@vercel/php"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/public/$1"
    }
  ],
  "build": {
    "env": {
      "APP_ENV": "production"
    }
  },
  "buildCommand": "npm run production",
  "outputDirectory": "public"
}