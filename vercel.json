{
  "version": 2,
  "builds": [
    {
      "src": "api/index.php",
      "use": "@vercel/php@0.6.0"
    }
  ],
  "routes": [
    { "src": "/(.*)", "dest": "/api/index.php" }
  ],
  "build": {
    "env": {
      "APP_ENV": "production"
    }
  },
  "buildCommand": "npm install && npm run production",
  "installCommand": "npm install",
  "framework": null,
  "outputDirectory": "public"
}